{
  "name": "jlondon-phpmyadmin",
  "version": "1.1.0",
  "author": "Justice London",
  "summary": "Puppet phpMyAdmin module",
  "license": "Apache License, Version 2.0",
  "source": "http://github.com/justicel/puppet-phpmyadmin.git",
  "project_page": "http://github.com/justicel/puppet-phpmyadmin",
  "issues_url": "http://github.com/justicel/puppet-phpmyadmin/issues",
