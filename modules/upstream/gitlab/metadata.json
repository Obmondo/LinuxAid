{
  "name": "puppet-gitlab",
  "version": "8.0.0",
  "author": "Vox Pupuli",
  "summary": "Installation and configuration of Gitlab Omnibus",
  "license": "BSD-3-Clause",
  "source": "https://github.com/voxpupuli/puppet-gitlab",
  "project_page": "https://github.com/voxpupuli/puppet-gitlab",
  "issues_url": "https://github.com/voxpupuli/puppet-gitlab/issues",
  "tags": [
    "git",
    "gitlab"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 9.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 4.4.0 < 9.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.1.0 < 8.0.0"
    }
  ]
}
