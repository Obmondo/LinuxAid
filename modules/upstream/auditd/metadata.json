{
  "name": "simp-auditd",
  "version": "8.14.5",
  "author": "SIMP Team",
  "summary": "A SIMP puppet module for managing auditd and audispd",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-auditd",
  "project_page": "https://github.com/simp/pupmod-simp-auditd",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "auditd",
    "audispd"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 8.0.0 < 10.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 4.9.0 < 5.0.0"
    },
    {
      "name": "puppet/augeasproviders_grub",
      "version_requirement": ">= 3.1.0 < 6.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
