<%- @add_header.sort.each do |header, value| -%>
    <%- if value -%>
      <%- if value.is_a?(Hash) -%>
        <%- value.each do |sk, sv| -%>
    add_header <%= header.inspect %> <% if sk != '' %><%= sk.inspect %><% end %> <%= sv %>;
        <%- end -%>
      <%- else -%>
    add_header <%= header.inspect %> <%= value.inspect %>;
      <%- end -%>
    <%- end -%>
<%- end -%>
